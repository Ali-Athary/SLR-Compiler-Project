topic "Advancements in Renewable Energy"
search {
    query ["renewable energy", "solar power", "wind energy"]
    year_range 2010 to 2023
    max_results 18
}
filters {
    exclude_keywords ["advertisement", "promotional"]
    min_pages 9
    languages ["en", "fa"]
}
analyze {
    summarize {
        method "gpt-4"
        max_length 400
    }
    extract_fields ["Introduction", "Findings"]
    keyword_cloud false
}
report {
    format "pdf"
    include_metadata true
    output_path "renewable_energy_advancements.pdf"
}